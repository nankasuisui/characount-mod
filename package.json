{
	"name": "selectedwordcount",
	"displayName": "Selected Word Count",
	"version": "0.1.0",
	"publisher": "nankasuisui",
	"description": "Markdown Word Count Example - a status bar contribution that reports out the number of words in a Markdown document as you interact with it.",
	"author": {
		"name": "nankasuisui"
	},
	"categories": [
		"Other"
	],
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:markdown",
		"onLanguage:plaintext"
	],
	"engines": {
		"vscode": "0.10.x"
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}
